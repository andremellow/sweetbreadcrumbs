{
    "preset": "laravel",
    "rules": {
        "phpdoc_trim": false,
        "phpdoc_no_empty_return": false,
        "phpdoc_align": true,
        "phpdoc_annotation_without_dot": false,
        "phpdoc_summary": true,
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_separation": true,
        "phpdoc_add_missing_param_annotation": true
    }
}